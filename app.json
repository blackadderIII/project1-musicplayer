{
  "expo": {
    "name": "musicplayer",
    "slug": "musicplayer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/musicIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscreen1.jpg",
      "resizeMode": "cover"
     
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
